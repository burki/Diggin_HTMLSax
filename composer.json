{
    "name": "diggin/diggin-htmlsax",
    "description": "FORK of PEAR's XML_HTMLSax3",
    "type": "library",
    "license": "LGPL-3.0",
    "keywords": [
        "sax", "HTMLSax"
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~7",
        "zendframework/zend-coding-standard": "^1.0"
    },
    "autoload": {
        "psr-4":{
            "Diggin\\HTMLSax\\":"src/Diggin/HTMLSax/"
        }
    },
    "autoload-dev": {
        "psr-4":{
            "DigginTest\\HTMLSax\\":"tests/DigginTest/HTMLSax/"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }
}
